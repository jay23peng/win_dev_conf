{
    "title": "jesse23 complex mapping for macOS",
    "rules": [
      {
        "description": "global vim key",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "open_bracket",
              "modifiers": {
                "mandatory": ["left_command"]
              }
            },
            "to": [
              {
                "key_code": "escape"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "w",
              "modifiers": {
                "mandatory": ["left_command"]
              }
            },
            "to": [
              {
                "key_code": "w",
                "modifiers": ["left_control"]
              }
            ]
          },
          {
            "type": "basic",
            "from": {
